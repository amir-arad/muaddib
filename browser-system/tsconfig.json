{
  "compilerOptions": {
    "lib": [
      "es2015", "es2016", "es2017", "dom"
    ],
    "module": "commonjs",
    "target": "es2015",
    "strict": true,
    "importHelpers": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
